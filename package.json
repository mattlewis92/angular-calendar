    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "lint-staged": "^6.1.0",
