    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "ts-node": "^3.0.0",
    "tslint": "^4.0.2",
    "tslint-eslint-rules": "^3.2.3",
    "tslint-loader": "^3.3.0",
    "typescript": "~2.2.1",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^2.8.2",
    "webpack": "^2.2.0",
    "webpack-angular-externals": "^1.0.0",
    "webpack-date-fns-externals": "^1.0.0",
    "webpack-dev-server": "^2.2.0",
