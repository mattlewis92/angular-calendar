    "sass-loader": "^7.0.3",
    "sinon": "^6.1.4",
    "sinon-chai": "^3.2.0",
    "standard-version": "^4.4.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-mwl": "^0.5.2",
