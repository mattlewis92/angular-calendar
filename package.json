    "karma-webpack": "^2.0.6",
    "lint-staged": "^5.0.0",
    "mocha": "^4.0.1",
    "moment": "^2.19.2",
    "ngx-contextmenu": "^1.3.5",
    "node-sass": "^4.7.1",
    "offline-plugin": "^4.8.4",
    "postcss-flexibility": "^2.0.0",
    "postcss-loader": "^2.0.5",
    "prettier": "^1.8.2",
    "raw-loader": "^0.5.1",
    "rrule": "^2.1.0",
    "rxjs": "^5.5.2",
    "sass-loader": "^6.0.2",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "standard-version": "^4.0.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-mwl": "^0.2.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "url-loader": "^0.6.2",
    "web-animations-js": "^2.3.1",
    "webpack": "^3.6.0",
    "webpack-angular-externals": "^1.0.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-date-fns-externals": "^1.0.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-filter-warnings-plugin": "^1.1.0",
    "webpack-rxjs-externals": "^1.1.0",
    "zone.js": "^0.8.18"
  },
  "peerDependencies": {
    "@angular/animations": ">=4.0.0 <6.0.0",
    "@angular/common": ">=4.0.0 <6.0.0",
    "@angular/core": ">=4.0.0 <6.0.0",
    "@angular/platform-browser": ">=4.0.0 <6.0.0",
    "typescript": "^2.3.0"
  },
  "dependencies": {
    "angular-draggable-droppable": "^1.1.1",
    "angular-resizable-element": "^1.2.4",
    "calendar-utils": "0.0.58",
    "date-fns": "^1.28.5",
    "positioning": "^1.3.1"
  }
}
