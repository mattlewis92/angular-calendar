  },
  "peerDependencies": {
    "@angular/animations": ">=6.0.0 <8.0.0",
    "@angular/common": ">=6.0.0 <8.0.0",
    "@angular/core": ">=6.0.0 <8.0.0"
  },
  "dependencies": {
    "angular-draggable-droppable": "^4.0.0-beta.16",
    "angular-resizable-element": "^3.2.0",
    "calendar-utils": "0.2.0-alpha.15",
    "positioning": "^1.4.0"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
