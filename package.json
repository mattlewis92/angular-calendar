{
  "name": "angular-calendar",
  "version": "0.22.0",
  "description": "A calendar component for angular 4.0+ that can display events on a month, week or day view",
  "main": "./umd/angular-calendar.js",
  "style": "./css/angular-calendar.css",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build:demos": "webpack --env=production",
    "build:umd": "webpack --config webpack.config.umd.ts",
    "build:ngc": "ngc -p tsconfig-ngc.json",
    "build:dist": "npm run build:umd && npm run build:ngc",
    "build:clean": "rm -rf dist",
    "test": "karma start --single-run && npm run build:dist && npm run build:clean",
    "test:watch": "karma start --auto-watch",
    "commit": "git-cz",
    "compodoc": "compodoc -p tsconfig-compodoc.json -d docs --disableGraph --disableCoverage --disablePrivate --disableInternal --disableLifeCycleHooks --disableProtected",
    "gh-pages": "git checkout gh-pages && git merge master --no-edit && rm -rf *.js *.js.map && npm run build:demos && npm run compodoc && git add . && git commit -m 'chore: build demos and docs' && git push && git checkout master",
    "copyfiles": "copyfiles package.json LICENSE README.md CHANGELOG.md dist && copyfiles -u 1 src/**/*.scss dist/scss",
    "prerelease": "npm test",
    "release:git": "git add package.json package-lock.json && git commit -m 'chore: bump version number' && standard-version --first-release && git push --follow-tags origin master",
    "release:npm": "npm run build:dist && npm run copyfiles && npm publish dist",
    "release": "npm run release:git && npm run release:npm",
    "postrelease": "npm run build:clean && npm run gh-pages",
    "codecov": "cat coverage/lcov.info | codecov",
    "prettier": "prettier --write",
    "commitmsg": "commitlint -e",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{demos,src,test}/**/*.ts": [
      "npm run prettier",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/angular-calendar.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "calendar"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/angular-calendar/issues"
  },
  "homepage": "https://github.com/mattlewis92/angular-calendar#readme",
  "devDependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/language-service": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@commitlint/cli": "^5.0.1",
    "@commitlint/config-angular": "^4.3.0",
    "@commitlint/prompt": "^5.0.1",
    "@compodoc/compodoc": "^1.0.4",
    "@mattlewis92/webpack-karma-die-hard": "^1.0.4",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngtools/webpack": "^1.8.2",
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "@types/rrule": "^2.1.7",
    "@types/sinon": "^4.0.0",
    "@types/sinon-chai": "^2.7.29",
    "@types/webpack": "^3.8.1",
    "angular-router-loader": "^0.8.0",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^7.1.6",
    "bootstrap": "4.0.0-beta",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "codelyzer": "^3.2.2",
    "commitizen": "^2.8.1",
    "concurrently": "^3.1.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.1",
    "create-plunker": "^1.2.0",
    "css-loader": "^0.28.7",
    "ejs-compiled-loader": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "hammerjs": "^2.0.8",
    "highlightjs-loader": "^0.2.3",
