    "postcss-flexibility": "^2.0.0",
    "prettier": "^1.10.2",
    "raw-loader": "^0.5.1",
    "rrule": "^2.1.0",
    "rxjs": "^5.5.3",
    "sass-loader": "^6.0.2",
    "sinon": "^4.2.3",
    "sinon-chai": "^2.14.0",
    "standard-version": "^4.0.0",
    "style-loader": "^0.20.1",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-webpack-plugin": "^0.10.1",
    "ts-loader": "^3.5.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-mwl": "^0.3.0",
    "tslint-loader": "^3.5.3",
